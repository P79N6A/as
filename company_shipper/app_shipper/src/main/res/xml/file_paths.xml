<?xml version="1.0" encoding="utf-8"?>
<paths>
    <!-- external-path:sd ；path:你的应用保存文件的根目录；name随便定义-->
    <!-- root-path 手机存储根目录 -->
    <external-path
        name="files_root"
        path="Android/data/com.yaoguang.shipper/" />
    <external-path
        name="external_storage_root"
        path="." />

    <files-path name="name" path="path" />
    <cache-path name="name" path="path" />
    <external-path name="name" path="path" />
    <external-files-path name="name" path="path" />
    <external-cache-path name="name" path="path" />

    <external-files-path name="video" path="/YGDriver/Video" />
    <external-files-path name="audio" path="/YGDriver/Audio/" />

    <external-path path="/driver/Images" name="camera_photos" />
    <external-path path="/YGDriver/Video" name="camera_photos" />
    <external-path path="/YGDriver/Audio" name="camera_photos" />

    <!-- /storage/emulated/0/Download/${applicationId}/.beta/apk-->
    <external-path name="beta_external_path" path="Download/"/>
    <!--/storage/emulated/0/Android/data/${applicationId}/files/apk/-->
    <external-path name="beta_external_files_path" path="Android/data/"/>

</paths>